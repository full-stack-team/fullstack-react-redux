{
    "version": "0.2.0",
    "configurations": [

    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "trace": "verbose",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceRoot}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via Babel",
      "protocol": "legacy",
      "program": "${workspaceRoot}/tools/srcServer.js",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
      "cwd": "${workspaceRoot}"
    }
  ]
  }